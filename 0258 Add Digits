class Solution(object):
    def addDigits(self, num):
        while len(str(num)) > 1:
            temp = 0
            for i in str(num):
                temp += int(i)
            num = temp
        return num

class Solution(object):
    def addDigits(self, num):
        return 9 if num % 9 == 0 and num != 0 else num % 9 #0(1) TIME
